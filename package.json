  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "mocha tests/index.js",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "aws:clean": "aws s3 rm s3://identiform --recursive --profile ReactS3",
    "aws:push": "aws s3 cp ./build s3://identiform  --recursive --cache-control public,max-age=604800 --profile ReactS3",
    "aws:clean:test": "aws s3 rm s3://identiform --recursive --profile ReactS3 --dry-run",
    "aws:push:test": "aws s3 cp ./build s3://identiform  --recursive --cache-control public,max-age=604800 --profile ReactS3 --dry-run",
    "deploy:aws": "npm run build && npm run aws:clean && npm run aws:push",
    "deploy:test": "npm run build && npm run aws:clean:test && npm run aws:push:test",
    "load": "artillery run loadtest.yml",
    "deploy": "git push production master"
  },
  "devDependencies": {
    "artillery": "^1.6.0-26",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "pkg-ok": "^2.3.1",
    "react-testing-library": "^5.3.2",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.6.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "webpack": "^4.27.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pkg-ok && npm test",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
