  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "bootstrap": "^4.1.1",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "d3-dsv": "^1.0.8",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "jsonp": "^0.2.1",
    "please-upgrade-node": "^3.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "react-spinners": "^0.3.2",
    "react-stockcharts": "^0.7.6",
    "react-validation": "^3.0.7",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "mocha tests/index.js",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record",
    "aws:clean": "aws s3 rm s3://identiform --recursive --profile ReactS3",
    "aws:push": "aws s3 cp ./build s3://identiform  --recursive --cache-control public,max-age=604800 --profile ReactS3",
    "aws:clean:test": "aws s3 rm s3://identiform --recursive --profile ReactS3 --dry-run",
    "aws:push:test": "aws s3 cp ./build s3://identiform  --recursive --cache-control public,max-age=604800 --profile ReactS3 --dry-run",
    "deploy": "npm run build && npm run aws:clean && npm run aws:push",
    "deploy:test": "npm run build && npm run aws:clean:test && npm run aws:push:test",
    "load": "artillery run loadtest.yml"
  },
  "devDependencies": {
    "artillery": "^1.6.0-26",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-import": "^1.7.0",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "husky": "^1.2.0",
    "jest": "^22.4.3",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "pkg-ok": "^2.3.1",
    "react-testing-library": "^3.0.2",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^7.0.1",
    "source-map-explorer": "^1.5.0",
    "style-loader": "^0.21.0",
    "styled-components": "^3.3.0",
    "webpack": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pkg-ok && npm test",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=11"
  }
}
